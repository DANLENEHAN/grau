{
    "configurations": [
        {
            "console": "integratedTerminal",
            "justMyCode": true,
            "name": "Python: Current File",
            "program": "${file}",
            "request": "launch",
            "type": "python"
        },
        {
            "args": [
                "run",
                "--host=0.0.0.0",
                "--debugger"
            ],
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_DEBUG": "1",
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            },
            "jinja": true,
            "justMyCode": true,
            "module": "flask",
            "name": "Python: Flask",
            "request": "launch",
            "type": "python"
        }
    ],
    "version": "0.2.0"
}
